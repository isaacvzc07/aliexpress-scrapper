# Shopify API credentials
SHOPIFY_API_KEY=your_api_key_here
SHOPIFY_PASSWORD=your_password_here

# Set your shop subdomain (without .myshopify.com), e.g., 'mi-tienda'
SHOPIFY_SHOP=your_shop_subdomain

# Optional: If you use a custom app with access token, set it here
SHOPIFY_ACCESS_TOKEN=your_access_token_here

# Admin API version (usar una versión estable)
SHOPIFY_API_VERSION=2024-10

# API Key de OpenAI (requerida para análisis de imágenes)
OPENAI_API_KEY=your_openai_api_key_here
